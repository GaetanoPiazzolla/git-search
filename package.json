{
  "name": "git-search",
  "version": "0.2.0",
  "private": true,
  "devDependencies": {
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "foreman": "^3.0.1"
  },
  "author": "Gaetano Piazzolla - gae.piaz@gmail.com",
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "bootstrap": "^5.1.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-loader-spinner": "^4.0.0",
    "react-scripts": "^5.0.0"
  },
  "homepage": "./",
  "main": "electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && electron-builder build",
    "electron": "electron .",
    "dev": "nf start -p 3000"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "productName": "git-search",
    "appId": "gae.piaz.git-search",
    "extends": null,
    "extraFiles": [
      {
        "from": "build",
        "to": "resources/build",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
